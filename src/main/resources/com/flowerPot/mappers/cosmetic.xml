<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flowerPot.cosmetic.repository.CosmeticDao">
	
	<insert id="insertCosmetic">
		insert into f_cosmetic values (cno_seq.nextval,#{type},#{brand},#{price},#{tag},#{discountPersent},#{stockNumber},#{skinType})
		<selectKey resultType="int" order="AFTER" keyProperty="cno">
			select cno from f_cosmetic
		</selectKey>
	</insert>	
	
	<delete id="deleteAll">
		delete from f_cosmetic
	</delete>
	
	<select id="selectCount" resultType="int">
		select count(*) from f_cosmetic
	</select>
</mapper>
