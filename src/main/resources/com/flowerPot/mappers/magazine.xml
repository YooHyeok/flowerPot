<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="magazine">
	<insert id="insertMagazine">
		insert into magazine (mgno,title,content,category,UUIDName,originalName,rootfolder,regdate) values 
		(mgno_seq.nextval, #{title}, #{content}, #{category}, #{UUIDName}, #{originalName}, #{rootfolder}, sysdate)
	</insert>
	
	<select id="selectMagazineList" resultType="com.flowerPot.vo.MagazineVo">
		select * from magazine
	</select>
</mapper>
